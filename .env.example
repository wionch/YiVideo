# Hugging Face Token
# 获取地址：https://huggingface.co/settings/tokens
# 用于免费的 community-1 模型 (当 config.yml 中 use_paid_api: false 时需要)
HF_TOKEN=your_huggingface_token_here

# PyannoteAI API Key
# 获取地址：https://pyannote.ai/
# 用于付费的 precision-2 模型 (当 config.yml 中 use_paid_api: true 时需要)
PYANNOTEAI_API_KEY=your_pyannoteai_api_key_here

# ========================================
# Redis Configuration
# ========================================
# Redis 主机地址
REDIS_HOST=host.docker.internal
# Redis 端口
REDIS_PORT=6379

# ========================================
# AI Providers API Keys
# ========================================
DEEPSEEK_API_KEY=your_deepseek_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
ZHIPU_API_KEY=your_zhipu_api_key_here
VOLCENGINE_API_KEY=your_volcengine_api_key_here
OPENAI_COMPATIBLE_API_KEY=your_openai_compatible_key_here